package main

import "fmt"

func sum(a int, b int, c int) int {
	return a + b + c
}

type Coche struct {
	puertas int
}

func (c *Coche) incrementPuertas() {
	c.puertas += 1
}

func main() {
	result := sum(1, 2, 3)
	fmt.Println(result)

	coche := Coche{}
	coche.incrementPuertas()
	fmt.Println(coche.puertas)
}
